    "@types/js-yaml": "^4.0.9",
    "@types/yargs": "^17.0.33",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "tsup": "^8.4.0",
    "yargs": "^17.7.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
